config := platformclientv2.GetDefaultConfiguration()
config.BasePath = ""    // expected format: https://api.mypurecloud.com
config.AccessToken = "your_access_token" // or use AuthorizeClientCredentials(...), AuthorizeCodeGrant(...) or AuthorizePKCEGrant(...)

apiInstance := platformclientv2.NewJourneyApiWithConfig(config)

var deploymentId string // The ID of the deployment sending the ping.
var customerCookieId string // ID of the customer associated with the ping.
var dl string // Document Location: 1) Web Page URL if overridden or URL fragment identifier (window.location.hash). OR  2) Application screen name that the ping request was sent from in the app. e.g. 'home' or 'help. Pings without this parameter will not return actions.
var dt string // Document Title.  A human readable name for the page or screen
var appNamespace string // Namespace of the application (e.g. com.genesys.bancodinero). Used for domain filtering in application sessions
var sessionId string // UUID of the customer session. Use the same Session Id for all pings, AppEvents and ActionEvents in the session
var sinceLastBeaconMilliseconds int // How long (milliseconds) since the last app event or beacon was sent. The response may return a pollInternvalMilliseconds to reduce the frequency of pings.
// Send a ping.
data, response, err := apiInstance.GetJourneyDeploymentCustomerPing(deploymentId, customerCookieId, dl, dt, appNamespace, sessionId, sinceLastBeaconMilliseconds)
if err != nil {
	fmt.Printf("Error calling GetJourneyDeploymentCustomerPing: %v\n", err)
} else {
	fmt.Printf("Response:\n  Success: %v\n  Status code: %v\n  Correlation ID: %v\n  Body: %v\n", response.IsSuccess, response.StatusCode, response.CorrelationID, data.String())
}