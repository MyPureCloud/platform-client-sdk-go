config.AccessToken = "your_access_token" // or use AuthorizeClientCredentials(...), AuthorizeCodeGrant(...) or AuthorizePKCEGrant(...)

apiInstance := platformclientv2.NewSettingsApiWithConfig(config)

var agentId string // The agent to apply the auto answer settings to
